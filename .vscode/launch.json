{
  // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
  // 기존 특성에 대한 설명을 보려면 가리킵니다.
  // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chainlit: v1",
      "type": "debugpy",
      "request": "launch",
      "module": "chainlit",
      "args": [
        "run",
        "agent_v1/chainlit/app.py",
        "--host",
        "127.0.0.1",
        "--port",
        "18001",
        "-w"
      ],
      "cwd": "${workspaceFolder}/src",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true
    },
    {
      "name": "Chainlit: v2",
      "type": "debugpy",
      "request": "launch",
      "module": "chainlit",
      "args": [
        "run",
        "agent_v2/chainlit/app.py",
        "--host",
        "127.0.0.1",
        "--port",
        "18001",
        "-w"
      ],
      "cwd": "${workspaceFolder}/src",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true
    },
    {
      "name": "Chainlit: v3",
      "type": "debugpy",
      "request": "launch",
      "module": "chainlit",
      "args": [
        "run",
        "agent_v3/chainlit/app.py",
        "--host",
        "127.0.0.1",
        "--port",
        "18001",
        "-w"
      ],
      "cwd": "${workspaceFolder}/src",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true
    },
    {
      "name": "Chainlit: v4",
      "type": "debugpy",
      "request": "launch",
      "module": "chainlit",
      "args": [
        "run",
        "agent_v4/chainlit/app.py",
        "--host",
        "127.0.0.1",
        "--port",
        "18001",
        "-w"
      ],
      "cwd": "${workspaceFolder}/src",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true
    },
    {
      "name": "Python: cli",
      "type": "debugpy",
      "request": "launch",
      "module": "cli",
      "cwd": "${workspaceFolder}/src",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Chainlit: run app.py",
      "type": "debugpy",
      "request": "launch",
      "module": "chainlit",
      "args": [
        "run",
        "ui/app.py",
        "--host",
        "127.0.0.1",
        "--port",
        "18001",
        "-w"
      ],
      "cwd": "${workspaceFolder}/src",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true
    },
    {
      "name": "Python: v3 ingest",
      "type": "debugpy",
      "request": "launch",
      "module": "agent_v3.scripts.ingest",
      "cwd": "${workspaceFolder}/src",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
