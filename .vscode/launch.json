{
  // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
  // 기존 특성에 대한 설명을 보려면 가리킵니다.
  // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: langsmith test",
      "type": "debugpy",
      "request": "launch",
      "module": "langsmith_test",
      "cwd": "${workspaceFolder}/src",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Python: cli",
      "type": "debugpy",
      "request": "launch",
      "module": "cli",
      "cwd": "${workspaceFolder}/src",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Chainlit: run app.py",
      "type": "debugpy",
      "request": "launch",
      "module": "chainlit",
      "args": ["run","src/app.py", "--host", "127.0.0.1", "--port", "18001", "-w"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "justMyCode": true
    }
    
  ]
}
